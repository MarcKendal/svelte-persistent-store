    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "rollup": "^2.9.1",
    "rollup-plugin-typescript2": "^0.27.0",
    "svelte": "^3.22.2",
    "ts-node": "^8.1w0.1",
    "tslib": "^1.11.2",
